<div class="card w-400 col align-items-center justify-content-center display-flex m-auto ">
  <div class="card-body">
    <h2 class="card-title">Create a Room</h2>
    <div>
      <%= form_tag "/rooms", method: :post do %>
      <%= label_tag :name, "Room Name" %>
      <%= text_field_tag :name %>
      <%= submit_tag "Create Room" %>
      <% end %>
    </div>
  </div>
</div>

